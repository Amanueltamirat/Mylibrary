<h2 class="search">Search Books</h2>
<form action="/books" method="GET" class="index-form">
  <label for="title">Title</label>
  <input type="text" name="title" value="<%= searchOptions.title %>" />
  <button class="search-button" type="submit">Search</button>
</form>
<br />

<% books.forEach((book)=>{ %>
<div>
  <h3>
    <a class="book-name" href="/books/<%= book.id  %>"
      >Book Title: <%= book.title %></a
    >
  </h3>
  <p class="book-description">Description: <%= book.description %></p>
</div>
<% }) %>
